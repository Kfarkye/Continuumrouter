# Supabase Configuration (Auto-populated in hosted environment)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# Gemini API Configuration
GEMINI_API_KEY=your-gemini-api-key-here
GEMINI_API_BASE=https://generativelanguage.googleapis.com/v1beta

# Gemini Pricing (USD per million tokens)
# Update these based on current Gemini pricing
GEMINI_RATE_IN_USD_PER_MTOK=1.25
GEMINI_RATE_OUT_USD_PER_MTOK=10.00

# Search API Configuration (Optional - for evidence gathering)
# Example: Brave Search API
SEARCH_API_URL=https://api.search.brave.com/res/v1/web/search
SEARCH_API_KEY=your-search-api-key-here

# External Reranker Configuration (Optional - for improved relevance scoring)
RERANKER_URL=https://your-reranker-service.com/rerank
RERANKER_API_KEY=your-reranker-api-key
